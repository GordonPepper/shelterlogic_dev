    <global>
        <models>
            <paymentterms>
                <class>Shelterlogic_PaymentTerms_Model</class>
                <resourceModel>paymentterms_resource</resourceModel>
            </paymentterms>
            <paymentterms_resource>
                <class>Shelterlogic_PaymentTerms_Model_Resource</class>
            </paymentterms_resource>
            <acimpro>
