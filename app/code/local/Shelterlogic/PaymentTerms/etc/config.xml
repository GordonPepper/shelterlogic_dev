                    <paymentMethod>Shelterlogic_PaymentTerms_Model_Rewrite_Acimpro_PaymentMethod</paymentMethod>
                </rewrite>
            </acimpro>
        </models>
        <blocks>
            <paymentterms>
                <class>Shelterlogic_PaymentTerms_Block</class>
            </paymentterms>
        </blocks>
        <helpers>
            <paymentterms>
                <class>Shelterlogic_PaymentTerms_Helper</class>
            </paymentterms>
        </helpers>
    </global>
    <default>
        <shelterlogic>
            <payment_terms>
                <net><![CDATA[NET10
NET30
NET60]]></net>
                <creditcard><![CDATA[CREDIT CARD]]></creditcard>
