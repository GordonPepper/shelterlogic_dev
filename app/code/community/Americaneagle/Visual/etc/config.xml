<?xml version="1.0"?>
<config>
    <modules>
        <Americaneagle_Visual>
            <version>1.0.0</version>
        </Americaneagle_Visual>
    </modules>
    <global>
        <helpers>
            <americaneagle_visual>
                <class>Americaneagle_Visual_Helper</class>
            </americaneagle_visual>
        </helpers>
        <models>
            <americaneagle_visual>
                <class>Americaneagle_Visual_Model</class>
            </americaneagle_visual>
        </models>
        <resources>
            <americaneagle_visual_setup>
                <setup>
                    <module>Americaneagle_Visual</module>
                    <class>Mage_Sales_Model_Resource_Setup</class>
                </setup>
            </americaneagle_visual_setup>
        </resources>
    </global>
    <crontab>
        <jobs>
            <americaneagle_visual>
                <schedule><cron_expr>*/5 * * * *</cron_expr></schedule>
                <run><model>americaneagle_visual/observer::pushOrders</model></run>
            </americaneagle_visual>
        </jobs>
    </crontab>
</config>