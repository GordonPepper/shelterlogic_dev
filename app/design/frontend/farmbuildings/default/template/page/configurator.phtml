                "14" => [],
                "13" => [],
                "0"  => []
            ];
            if ($handle = opendir(Mage::getBaseDir().'/media/wysiwyg/sp-shelter/thumbs/')) {

                /* This is the correct way to loop over the directory. */
                while (false !== ($entry = readdir($handle))) {
                    if(!empty(explode("-",$entry)[1])) {
                        $stack_index = (explode("-",$entry)[0] == 'BN' ? '12' : (explode("-",$entry)[0] == 'PK' ? '14' : (explode("-",$entry)[0] == 'RD' ? '13' : '0')));
                        array_push($image_stack[$stack_index], ["thumb" => $this->getBaseUrl().'/media/wysiwyg/sp-shelter/thumbs/'.$entry, "full" => $this->getBaseUrl().'/media/wysiwyg/sp-shelter/full/'.$entry]);
                        array_push($image_stack['0'], ["thumb" => $this->getBaseUrl().'/media/wysiwyg/sp-shelter/thumbs/'.$entry, "full" => $this->getBaseUrl().'/media/wysiwyg/sp-shelter/full/'.$entry]);
                    }
                }

                closedir($handle);
            }

        ?>
        <script> var image_stack = <?php echo json_encode($image_stack); ?>; </script>

        <?php echo $this->getChildHtml('modal_includes') ?>
        
        <?php echo $this->getChildHtml('before_body_end') ?>

        <?php echo $this->getAbsoluteFooter() ?>

    </div>
    
</body>
</html>
